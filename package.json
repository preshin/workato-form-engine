{
  "name": "react-formio-engine",
  "version": "1.0.7",
  "description": "Form engine with builder and renderer - formiojs-powered with React functional components",
  "type": "module",
  "main": "dist/form-engine.umd.js",
  "module": "dist/form-engine.es.js",
  "exports": {
    ".": {
      "import": "./dist/form-engine.es.js",
      "require": "./dist/form-engine.umd.js"
    },
    "./themes/default.css": "./src/themes/default.css",
    "./themes/antd.css": "./src/themes/antd.css"
  },
  "files": [
    "dist",
    "src/themes"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.3",
    "bootstrap": "^4.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "vite": "^7.3.1"
  },
  "keywords": [
    "react",
    "formio",
    "form-builder",
    "form-renderer",
    "json-schema-form",
    "drag-and-drop",
    "form-engine",
    "formiojs",
    "functional-components"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/preshin/react-formio-engine.git"
  },
  "homepage": "https://github.com/preshin/react-formio-engine#readme",
  "bugs": {
    "url": "https://github.com/preshin/react-formio-engine/issues"
  },
  "license": "MIT",
  "dependencies": {
    "formiojs": "^4.21.7"
  }
}
